@page
@model ukol_15_3_2018.SingedModel
<div class="container row p-0 m-0">
    <h1>Signed</h1>
    @if (!String.IsNullOrEmpty(Model.SuccessMessage))
    {
        <div class="alert alert-success">@Model.SuccessMessage</div>
    }
    @if (!String.IsNullOrEmpty(Model.FailureMessage))
    {
        <div class="alert alert-danger">@Model.FailureMessage</div>
    }
    <div class="col-3 offset-3">
        <a asp-action="Adding" asp-controller="Notes" class="btn btn-info form-control text-white">
            Add new note
        </a>
    </div>
    <div class="col-12 border p-1">
        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>Name : @Model.Author</th>
                </tr>
            </thead>
        </table>



    </div>







</div>

